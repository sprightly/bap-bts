    entity_class: 'AppBundle\\Entity\\Issue',
    exportProcessor: 'app_issue.export',
    exportTitle: 'app.issue.operation.export'|trans,
    importProcessor: 'app_issue.add_or_replace',
    importTitle: 'app.issue.operation.import'|trans,
    dataGridName: gridName
    } %}

    {{ parent() }}
{% endblock %}